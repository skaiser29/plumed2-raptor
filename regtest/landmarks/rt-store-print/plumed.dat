d1: READ FILE=colv_in VALUES=data

ff1: COLLECT_FRAMES STRIDE=1 ARG=d1 CLEAR=6
DUMPVECTOR ARG=ff1.* FILE=output-half STRIDE=6
ff2: COLLECT_FRAMES STRIDE=1 ARG=d1
DUMPVECTOR ARG=ff2.* FILE=output-all 
DUMPVECTOR ARG=ff2.* FILE=output-reuse
 
ll1: LANDMARK_SELECT_STRIDE ARG=ff2 NLANDMARKS=5 NOVORONOI NODISSIMILARITIES
DUMPVECTOR ARG=ll1.* FILE=output-stride 

ll2: LANDMARK_SELECT_STRIDE ARG=ff1 NLANDMARKS=3 NOVORONOI NODISSIMILARITIES
DUMPVECTOR ARG=ll2.* FILE=output-stride2 STRIDE=6

ll3: LANDMARK_SELECT_RANDOM ARG=ff1 NLANDMARKS=4 SEED=230623 NOVORONOI NODISSIMILARITIES
DUMPVECTOR ARG=ll3.* FILE=output-stride3 STRIDE=6
